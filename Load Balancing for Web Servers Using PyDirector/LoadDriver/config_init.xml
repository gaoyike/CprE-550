<!--config.xml for Load Driver -->
<rootconfig>
 <rate>1</rate> <!--rate is defined as requests per second -->
 <period>1</period> <!-- period is the number of seconds to send requests -->
 <timeout>1</timeout> <!-- timeout is the number of seconds to wait for the last requests to finish-->
 <host>
   <hostname>www.google.com</hostname>
   <port>80</port>
 </host>
 <host>
   <hostname>www.iastate.edu</hostname>
   <port>80</port>
 </host>
</rootconfig>
